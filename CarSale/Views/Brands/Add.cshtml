@model Brand

@{
    ViewBag.Title = "Добавить брэнд";
    Layout = "_Layout";
}

<h2 class="text-center">@ViewBag.Title</h2>
@if (ViewBag.ErrorExist != null || ViewBag.ErrorCreateDate != null)
{
    <h4 class="text-danger text-center">@ViewBag.ErrorExist</h4>
    <h4 class="text-danger text-center">@ViewBag.ErrorCreateDate</h4>
}
<div class="row justify-content-center">

    <div class="phone_add_form col-md-6">
        <form asp-action="Add" method="post">

            @await Html.PartialAsync("PartialViews/BrandFormPartial", new Brand())
            <div class="text-center">
                <button type="submit" class="btn btn-outline-info mt-3">Создать</button>
            </div>
        </form>

        @section Scripts

        {

            @{
                await Html.RenderPartialAsync("_ValidationScriptsPartial");

            }

        }

    </div>

</div>